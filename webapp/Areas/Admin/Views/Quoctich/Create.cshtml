@model webapp.Models.Quoctich

@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div class="container">
    <h1>Thêm mới quốc tịch</h1>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Tenqt" class="control-label">Tên quốc tịch</label>
                    <input asp-for="Tenqt" class="form-control" />
                    <span asp-validation-for="Tenqt" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                    <a class="btn btn-warning" asp-action="Index">Back to List</a>
                </div>
            </form>
        </div>
    </div>
</div>

