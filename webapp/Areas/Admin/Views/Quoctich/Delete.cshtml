@model webapp.Models.Quoctich

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div class="container">
    <h1>Xóa quốc tịch</h1>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Tên quốc tịch
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Tenqt)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Maqt" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a class="btn btn-warning" asp-action="Index">Back to List</a>
    </form>
</div>
