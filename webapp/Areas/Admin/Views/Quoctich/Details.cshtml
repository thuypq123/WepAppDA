@model webapp.Models.Quoctich

@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}


<div class="container">
    <h1>Chi tiết quốc tịch</h1>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Tên quốc tịch
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Tenqt)
        </dd>
    </dl>
    <div>
        <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Maqt">Edit</a> |
        <a class="btn btn-warning" asp-action="Index">Back to List</a>
    </div>
</div>

