@model IEnumerable<webapp.Models.Dantoc>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/Admin.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tendt)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tendt)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Madt">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Madt">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Madt">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
